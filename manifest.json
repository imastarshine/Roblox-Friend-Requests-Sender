{
  "manifest_version": 3,
  "name": "Roblox Friend Request Sender",
  "version": "1.0",
  "description": "EZ",
  "permissions": [
      "storage"
  ],
  "host_permissions": [
    "https://www.roblox.com/*",
    "https://friends.roblox.com/*",
    "https://users.roblox.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}